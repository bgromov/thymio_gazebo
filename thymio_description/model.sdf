<?xml version="1.0"?>
<sdf version="1.4">
  <model name="thymio">
    <!--static>true</static-->
    <pose>0 0 0.0305 0 0 0</pose>
    <link name="body">
      <inertial>
        <mass>0.200</mass>
        <inertia>
          <!-- box inertials -->
          <!-- ixx = mass * (y*y + z*z) / 12 -->
          <ixx>0.00024282</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <!-- iyy = mass * (x*x + z*z) / 12 -->
          <iyy>0.00023542</iyy>
          <iyz>0.0</iyz>
          <!-- izz = mass * (x*x + y*y) / 12 -->
          <izz>0.00041073</izz>
        </inertia>
      </inertial>
      <self_collide>true</self_collide>
      <collision name="collision_body">
        <pose>0 0 0.0225 0 0 0</pose>
        <geometry>
          <box>
            <size>0.110 0.112 0.045</size>
          </box>
        </geometry>
      </collision>
      <collision name="collision_caster">
        <pose>0.035 0 0 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>0.009</radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual_body">
        <geometry>
          <mesh>
            <uri>model://thymio/meshes/thymio_body.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <visual name="visual_proximity_sensors">
        <material>
          <ambient>0.03 0.03 0.03 1.0</ambient>
          <diffuse>0.3 0.3 0.3 1.0</diffuse>
        </material>
        <geometry>
          <mesh>
            <uri>model://thymio/meshes/thymio_proximity_sensors.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    <link name="left_wheel">
      <pose>-0.025 0.047 0.013 0 0 0</pose>
      <inertial>
        <mass>0.025</mass>
        <inertia>
          <!-- cylinder along Y-axis -->
          <!-- ixx = mass * ( h*h / 12 + r*r / 4) -->
          <ixx>0.00000363</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <!-- iyy = mass * (r*r) / 2 -->
          <iyy>0.00000605</iyy>
          <iyz>0.0</iyz>
          <!-- izz = mass * ( h*h / 12 + r*r / 4) -->
          <izz>0.00000363</izz>
        </inertia>
      </inertial>
      <collision name="collision_left_wheel">
        <pose>0 0.00075 0 1.5707 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.022</radius>
            <length>0.017</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual_left_wheel">
        <material>
          <ambient>0.03 0.03 0.03 1.0</ambient>
          <diffuse>0.3 0.3 0.3 1.0</diffuse>
        </material>
        <geometry>
          <mesh>
            <uri>model://thymio/meshes/thymio_left_wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    <link name="right_wheel">
      <pose>-0.025 -0.047 0.013 0 0 0</pose>
      <inertial>
        <mass>0.025</mass>
        <inertia>
          <!-- cylinder along Y-axis -->
          <!-- ixx = mass * ( h*h / 12 + r*r / 4) -->
          <ixx>0.00000363</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <!-- iyy = mass * (r*r) / 2 -->
          <iyy>0.00000605</iyy>
          <iyz>0.0</iyz>
          <!-- izz = mass * ( h*h / 12 + r*r / 4) -->
          <izz>0.00000363</izz>
        </inertia>
      </inertial>
      <collision name="collision_right_wheel">
        <pose>0 -0.00075 0 1.5707 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.022</radius>
            <length>0.017</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual_right_wheel">
        <material>
          <ambient>0.03 0.03 0.03 1.0</ambient>
          <diffuse>0.3 0.3 0.3 1.0</diffuse>
        </material>
        <geometry>
          <mesh>
            <uri>model://thymio/meshes/thymio_right_wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    <joint type="revolute" name="left_wheel_hinge">
      <!--pose>-0.025 -0.04775 0.013</pose-->
      <child>left_wheel</child>
      <parent>body</parent>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>
    <joint type="revolute" name="right_wheel_hinge">
      <!--pose>-0.025 0.04775 0.013</pose-->
      <child>right_wheel</child>
      <parent>body</parent>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>
  </model>
</sdf>
